ALGORITHM somme de deux tables
VAR
    tab1,tab2:=INTEGER:=0;
    i,j,som:=INTEGER:=0;
BEGIN

    REPEAT
        read (tab[i]);
        read (tab[j]);
        i:=i+1;
        j:=j+1;
    UNTIL tab1[i]=0 or set2[j]=0;
        FOR i FROM 0 TO LENGTH tab1[i]  DO
        FOR j FROM 0 TO LENGTH tab2[j]  DO
            som:=tab1[i]+tab2[j];
        END_FOR
        write (som);
END